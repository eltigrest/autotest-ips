const sum = (a, b) => {
    console.log('sum', a + b)
}
const diff = (a, b) => {
    console.log('difference', a - b)
}
const multip = (a, b) => {
    console.log('multiply', a * b)
}
const div = (a, b) => {
    console.log('division', a / b)
}

const calc = (a, b, callback) => {
    console.log('Result for', a, b)
    callback(a, b)
}

let num1 = 45
let num2 = 9
calc(num1, num2, sum)
calc(num1, num2, diff)
calc(num1, num2, multip)
calc(num1, num2, div)